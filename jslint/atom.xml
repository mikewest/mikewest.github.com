 <author>
   <name>Mike West</name>
   <email>mike@mikewest.org</email>
 </author>

 {% for post in site.categories.jslint %}
 <entry>
   <title>Changelog: {{ post.date | date: "%Y-%m-%d" }}</title>
   <link href="http://projects.mikewest.org/jslint/"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://projects.mikewest.org/jslint{{ post.id }}</id>
   <content type="html">&lt;strong&gt;{{ post.date | date: "%Y-%m-%d" }} (Based on JSLint from {{ post.version }})&lt;/strong&gt;: {{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
